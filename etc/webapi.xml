<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route method="GET" url="/V1/productParent">
        <service class="Squeezely\Plugin\Api\GetManagementInterface" method="getParentIdOfProduct"/>
        <resources>
            <resource ref="Magento_Backend::admin"/>
        </resources>
    </route>
    <route method="POST" url="/V1/productInfo">
        <service class="Squeezely\Plugin\Api\GetManagementInterface" method="getProductsInfo"/>
        <resources>
            <resource ref="Magento_Backend::admin"/>
        </resources>
    </route>
</routes>